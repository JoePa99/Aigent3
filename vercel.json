{
  "version": 2,
  "functions": {
    "api/*.ts": {
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "env": {
    "FIREBASE_PROJECT_ID": "aigencycursor",
    "FIREBASE_STORAGE_BUCKET": "aigencycursor.firebasestorage.app"
  }
} 